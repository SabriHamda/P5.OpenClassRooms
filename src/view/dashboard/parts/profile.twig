{% block profile %}
            <div class="thumbnail">
               <img src="public/assets/images/apropos.png" alt="image">
               <div class="caption">
                  <h3>Mon Profil</h3>
                  <p><strong>Statut :</strong> {{ session.role }}</p>
                  <p><strong>Nom :</strong> {{ session.prenom }}</p>
                  <p><strong>Email :</strong> {{ session.email }}</p>
                  <p><a href="#" class="btn btn-primary" role="button">Modifier</a> <a href="#" class="btn btn-default" role="button">Ajouter</a></p>
               </div>
            </div>
{% endblock profile %}         